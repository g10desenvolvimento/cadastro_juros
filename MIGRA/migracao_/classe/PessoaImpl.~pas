unit PessoaImpl;

interface
uses
  Dados;

type
  TPessoaImpl = class
  private
    { Private Declarations }
  public
    dados : TDados;
    codigo: integer;
    status: String;
  constructor Create;  

  end;

implementation

constructor TPessoaImpl.Create;
begin
  dados := TDados.Create;
  inherited Create;
end;

end.
